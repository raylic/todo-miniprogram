.container{
  background-color: #eeeeee;
  height: 100vh;
}
.userinfo {
  display: block;
  height: 160rpx;
  position: relative;
  background-color: #ffffff;
}
.userinfo-avatar {
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}
.userinfo-text {
  display: inline-block;
  font-size: 45rpx;
  position: absolute;
  top: 40rpx;
  left: 160rpx;
  color: rgb(38, 183, 169);
}
.days-text {
  font-size: 30rpx;
  position: absolute;
  top: 60rpx;
  right: 20rpx;
  color: rgb(38, 183, 169);
}
.date-text {
  position: relative;
  padding-bottom: 20rpx;
}
.date-text  text{
  background-color: #ffffff;
  width: 260rpx;
  font-size: 50rpx;
  height: 72rpx;
  border: 2px rgb(38, 183, 169) solid;
  display: inline-block;
  text-align: center;
  border-left: none;
  border-right: none;
  position: absolute;
  top: 20rpx;
  left: 0;
}
.right-border {
  height: 80rpx;
  width: 75rpx;
  border-left: 2px rgb(38, 183, 169) solid;
  border-radius: 30rpx;
  display: inline-block;
  position: relative;
  top: 20rpx;
  left: 240rpx;
  background-color: #eeeeee;
}
.date-month {
  text-align: center;
  font-size: 50rpx;
  color: rgb(38, 183, 169);
  background-color: #ffffff;
}
.calendar {
  padding: 0rpx 20rpx;
  justify-content: center;
  align-items: center;
  background-color: #ffffff;
}
#title {
  color: black;
  background-color: transparent;
}
.single-day {
  font-size: 26rpx;
  display: inline-block;
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  width: 50rpx;
  margin: 0 25rpx;
  border-radius: 50%;
  background-color: rgb(38, 183, 169);
  color: gray;
}
.notThisMonth {
  color: rgb(200,200,200);
}
#notFinish {
  background-color: transparent;
}
.logs {
  margin: 20rpx 0;
  margin-right: 10rpx;
  position: relative;
}
.logs .date {
  background-color: rgb(38, 183, 169);
  color: #ffffff;
  text-align: center;
  letter-spacing: 2rpx;
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 40rpx;
  font-weight: 400;
  width: 130rpx;
  position: absolute;
  top: 0;
  left: 1vw;
}

.plans {
  border-top: 2px rgb(38, 183, 169) solid;
  padding-top: 30rpx;
  margin: 0;
  width: 98vw;
  margin: 0 1vw;
  display: block;
  background-color: #ffffff;
  position: absolute;
  top: 50rpx;
  left: 0;
}
.line {
  margin-top: 20rpx;
  width: 98vw;
  height: 50rpx;
}
.single-plan {
  position: relative;
  margin: 0 0 10rpx 0;
  height: 70rpx;
  
}
.single-plan text {
  color: rgb(99,99,99);
  font-size: 45rpx;
  letter-spacing: 2rpx;
  position: absolute;
  left: 25rpx;
  top: 0rpx;
}
.single-plan .finish {
  position: absolute;
  right: 20rpx;
  width: 90rpx;
  height: 40rpx;
  font-size: 25rpx;
  top: 20rpx;
  text-align: center;
  border: 1px solid rgb(38,183,196);
  color: rgb(38,183,196);
  border-radius: 30rpx;
}
#unfinish {
  background-color: rgb(141,166,164);
  color: rgb(254,254,254);
  border: 1rpx transparent solid;
}